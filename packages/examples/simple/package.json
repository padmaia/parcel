{
  "name": "@parcel/simple-example",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "demo": "PARCEL_WORKERS=1 node -r @parcel/babel-register ./node_modules/.bin/parcel src/index.js",
    "clean-demo": "rm -rf .parcel-cache dist && yarn demo"
  },
  "devDependencies": {
    "@parcel/babel-register": "^2.0.0",
    "@parcel/core": "^2.0.0"
  },
  "browser": "dist/legacy/index.js",
  "browserModern": "dist/modern/index.js",
  "targets": {
    "browserModern": {
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    },
    "browser": {
      "engines": {
        "browsers": [
          "> 0.25%"
        ]
      }
    }
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
